<form role='form' id='newPortfolio'>
  <div class='form-group'>
    <label for='pfTitle'>Title</label>
    <input class="form-control" type='text' id='pfTitle'/>
  </div>

  <div class="form-group">
    <label for='pfDescription'>Description</label>
    <textarea rows="5" class='form-control' id='pfDescription'></textarea>
  </div>

  <div class='form-group'>
    <label for='pfImage'>Image</label>
    <input class="form-control" type='text' id='pfImage'/>
  </div>

  <button type="submit" class="btn btn-default">Submit</button>
</form>
